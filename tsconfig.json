{
  "compilerOptions": {
    "module": "es6",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "dist",
    "noEmit": false
  },
  "lib": [
    "es2016"
  ],
  "include": ["src/**/*"],
  "exclude": ["node_modules", "src/**/*.spec.ts"]
}