{
  "name": "polarsteps-unpacker",
  "version": "0.0.1",
  "description": "A demo app to show how NW.js works",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/src",
    "build-ts": "tsc",
    "pack": "npx webpack",
    "start": "node --experimental-specifier-resolution=node dist/src/app/Starter.js",
    "start-nw": "nw dist/",
    "build-nw": "nwbuild --platforms win64 --buildDir dist/ src/"
  },
  "dependencies": {
    "docx": "^7.6.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "lodash": "^4.17.21",
    "nw": "^0.71.0",
    "nw-builder": "^4.0.0-rc.8",
    "ts-loader": "^9.4.2",
    "tslint": "3.2.1",
    "typescript": "^4.9.4",
    "webpack": "^5.38.1",
    "webpack-cli": "^5.0.1"
  }
}
